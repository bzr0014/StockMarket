[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/bzr0014
[access "refs/for/refs/*"]
	read = group user/bzr0014
	push = group user/bzr0014
	label-Code-Review = -2..+2 group user/bzr0014
	label-Verified = -1..+1 group user/bzr0014
	rebase = group user/bzr0014
	removeReviewer = group user/bzr0014
	submit = group user/bzr0014
[access "refs/heads/*"]
	read = group user/bzr0014
	create = group user/bzr0014
	push = +force group user/bzr0014
	pushMerge = group user/bzr0014
[access "refs/tags/*"]
	push = +force group user/bzr0014
	pushTag = group user/bzr0014
	pushSignedTag = group user/bzr0014
